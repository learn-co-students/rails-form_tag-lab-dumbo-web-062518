<%= form_tag students_path do %>
  <label>Student Form:</label><br>
  <!-- <input type="text" id="student_form" name="student[form]"><br> -->

  <label>First name:</label><br>
  <!-- <textarea id="first_name" name="student[first_name]"></textarea><br> -->
  <%= text_area_tag :'first_name'%><br>

  <label>Last name:</label><br>
  <!-- <textarea id= "last_name" name="student[last_name]"></textarea><br> -->
  <%= text_area_tag :'last_name'%><br>

